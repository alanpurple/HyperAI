{
  "compileOnSave": true,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./",
    "module": "Preserve",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "lib": [ "ES2022" ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ]
}
